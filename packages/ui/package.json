{
  "name": "@vibetree/ui",
  "version": "0.0.1",
  "description": "Shared UI components for VibeTree",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "node build.js",
    "build:tsc": "tsc",
    "dev": "tsc -w",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@vibetree/core": "workspace:^",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.16.0-beta.118",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "5.6.0-beta.118",
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.9",
    "jsdom": "^27.0.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}